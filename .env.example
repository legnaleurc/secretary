# (required) Telegram Bot API token
API_TOKEN="telegram bot api token"

# (optional) Will use webhook if all presents, otherwise use polling.
# Listening hosts seperated by spaces.
LISTEN_LIST="127.0.0.1:8080 [::1]:8000"
# External URL for Telegram webhook.
WEBHOOK_URL="https://my.public.host/path/to/webhook"
# Internal path for webhook.
WEBHOOK_PATH="/path/to/webhook"

# (optional) Secret used for verifying incoming requests.
CLIENT_TOKEN="my secret"

# (optional)
DVD_LIST="name1|https://dvd1 name2|https://dvd2"

# (optional)
DULD_ORIGIN="https://duld"

# (optional)
TORRENT_URL="https://torrent"

# (required if using Docker Compose)
HOST_PORT="1234"
CONTAINER_PORT="5678"
